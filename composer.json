{
    "name": "php-luminous/php-luminous",
    "description": "Joshdev Php framework",
    "type": "project",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Core\\": "Core/",
            "App\\": "app/",
            "Database\\Generators\\": "database/generators/",
            "Database\\Dummy\\": "database/dummy/"
        },
        "files": [
            "Core/Support/helpers.php",
            "Core/Support/CollectionHelpers.php",
            "Core/Routing/helpers.php",
            "Core/Http/helpers.php",
            "Core/Filesystem/functions.php",
            "Core/View/view_helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "prefer-stable": true,
    "minimum-stability": "dev",
    
    "require": {
        "psr/container": "^2.0",
        "symfony/var-dumper": "^7.3",
        "vlucas/phpdotenv": "^5.6",
        "filp/whoops": "^2.18",
        "ramsey/uuid": "^4.9",
        "nesbot/carbon": "^3.10",
        "symfony/uid": "^7.3",
        "fakerphp/faker": "^1.24",
        "symfony/rate-limiter": "^7.3",
        "symfony/cache": "^7.3",
        "symfony/http-foundation": "^7.3",
        "symfony/error-handler": "^7.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^12.4",
        "mockery/mockery": "^1.6"
    }
}
